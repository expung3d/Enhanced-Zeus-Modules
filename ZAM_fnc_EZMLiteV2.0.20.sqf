    if(_var == 1) exitWith {1}; 
    comment "Get animationSourcePhase from door, if closed return 0, if open return 2."; 
    [0,2] select (_building animationSourcePhase (format ["door_%1_sound_source", _door]) > 0.5) 
   }; 
 
