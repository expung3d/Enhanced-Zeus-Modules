					}forEach _objects;

					{
						_x params ["_object","_objectPos"];
						_objects deleteAt (_objects find _x);
						deleteVehicle _object;
					}forEach _deleteObjects;

					_unit setVariable ["MAZ_EZM_CompObjects",_objects,true];
				};
			}];

			if(isNil "MAZ_EZM_buildingDestructSetup") then {
